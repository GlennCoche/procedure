{
  "name": "procedures-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:persistent": "nohup next dev > ../.next-server.log 2>&1 &",
    "server:start": "../scripts/start-server.sh start",
    "server:stop": "../scripts/start-server.sh stop",
    "server:restart": "../scripts/start-server.sh restart",
    "server:status": "../scripts/start-server.sh status",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "test": "cd ../tests && tsx cli.ts",
    "test:all": "cd ../tests && tsx cli.ts",
    "test:local": "cd ../tests && tsx cli.ts --env local",
    "test:prod": "cd ../tests && tsx cli.ts --env production",
    "test:auth": "cd ../tests && tsx cli.ts --suite auth",
    "test:procedures": "cd ../tests && tsx cli.ts --suite procedures",
    "test:executions": "cd ../tests && tsx cli.ts --suite executions",
    "test:api": "vitest run",
    "test:e2e": "playwright test",
    "test:fix": "cd ../tests && tsx cli.ts --fix",
    "test:report": "cd ../tests && tsx generate-report.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.17.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@xyflow/react": "^12.0.0",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "next": "^14.2.0",
    "next-auth": "^4.24.0",
    "openai": "^4.20.0",
    "postcss": "^8.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-speech-recognition": "^3.10.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^25.0.7",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "happy-dom": "^20.1.0",
    "playwright": "^1.57.0",
    "prisma": "^5.7.0",
    "tsx": "^4.21.0",
    "vitest": "^4.0.17"
  }
}
